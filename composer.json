{
  "name": "cawolf/php-dependency-analysis-suite",
  "description": "Suite helping you to integrate the great project PhpDependencyAnalysis into your application",
  "keywords": [
    "dependency", "graph", "analysis", "architecture", "phpda", "suite"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Christian Alexander Wolf",
      "email": "mail@cawolf.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cawolf\\PhpDependencyAnalysisSuite\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cawolf\\PhpDependencyAnalysisSuite\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.5",
    "symfony/console": "^2.5|^3.0",
    "symfony/yaml": "^2.5|^3.0",
    "symfony/serializer": "^2.5|^3.0",
    "mamuz/php-dependency-analysis": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.36",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "bin": [
    "bin/phpdasuite"
  ],
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
