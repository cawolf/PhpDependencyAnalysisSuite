{
  "name": "cawolf/php-dependency-analysis-suite",
  "license": "MIT",
  "type": "project",
  "version": "1.0",
  "autoload": {
    "psr-4": {
      "Cawolf\\PhpDependencyAnalysisSuite\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cawolf\\PhpDependencyAnalysisSuite\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.5.9",
    "symfony/console": ">=2.5",
    "symfony/yaml": ">=2.5",
    "symfony/serializer": ">=2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "bin": [
    "bin/phpdasuite"
  ],
  "suggest": {
    "mamuz/php-dependency-analysis": "This project is the reason for the suite."
  }
}
